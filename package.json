  "bugs": {
    "url": "https://github.com/ArashSarikhani/tailwindcss-react-data-table/issues"
  },
  "homepage": "https://github.com/ArashSarikhani/tailwindcss-react-data-table#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
